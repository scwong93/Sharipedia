<h1>All Wikis<h1>

<% if authorize_create %>
  <div class="row">
    <div class="col-md-4">
      <%= link_to "Create Wiki", new_wiki_path, class: 'btn btn-success' %>
    </div>
  </div>
<% end %>

<% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki %>
      </h4>
    </div>
  </div>
<% end %>
